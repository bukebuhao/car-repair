        <tr>
		<th>模块名称</th>
		<th>控制器</th>
		<th>版本</th>
		<th width="80">操作</th>
		</tr>
         <volist name="install" id="vo">
		
		<td>{$vo.module_title}(<strong>{$vo.module_name}</strong>) <eq name="vo['system_module']" value="1"><img src="__PUBLIC__/Admin/images/lock.gif" alt="内置模块锁定状态" align="absmiddle" /></eq></td>
		<td>{$vo.module_name}Action.class.php</td>
		<td>{$vo.build_version}</td>
		<td>
		  <a href="{:U("Module/modify",array('id'=>$vo['id']))}">编辑</a> <!--  <a href="{:U("Module/uninstall",array('id'=>$vo['id']))}" >卸载</a>-->
		  
		  </td>
		</tr>
		</volist>
        <tr>
		  <th colspan="4" class="style1" style="font-size:14px">系统内置</th>
		  </tr>
         
          <volist name="system" id="vo">
            <td>{$vo.module_title}(<strong>{$vo.module_name}</strong>) <eq name="vo['system_module']" value="1"><img src="__PUBLIC__/Admin/images/lock.gif" alt="内置模块锁定状态" align="absmiddle" /></eq></td>
              <td>{$vo.module_name}Action.class.php</td>
              <td>{$vo.build_version}</td>
              <td><a href="{:U("Module/modify",array('id'=>$vo['id']))}">查看</a> 
              </td>
              </tr>
          </volist>
